{
  "name": "custom-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "markdown",
          "global",
          "all"
        ],
        "path": "./global.json"
      },
      {
        "language": [
          "license"
        ],
        "path": "./license.json"
      },
      {
        "language": "c",
        "path": "./c/c.json"
      },
      {
        "language": "cdoc",
        "path": "./c/cdoc.json"
      },
      {
        "language": "cpp",
        "path": "./cpp/cpp.json"
      },
      {
        "language": "cppdoc",
        "path": "./cpp/cppdoc.json"
      },
      {
        "language": "editorconfig",
        "path": "./editorconfig.json"
      },
      {
        "language": [
          "gitcommit",
          "NeogitCommitMessage"
        ],
        "path": "./gitcommit.json"
      },
      {
        "language": "lua",
        "path": "./lua/lua.json"
      },
      {
        "language": "luadoc",
        "path": "./lua/luadoc.json"
      },
      {
        "language": "sql",
        "path": "./sql.json"
      },
      {
        "language": [
          "shellscript",
          "shell",
          "sh",
          "zsh"
        ],
        "path": "./shell/shell.json"
      },
      {
        "language": "shelldoc",
        "path": "./shell/shelldoc.json"
      },
      {
        "language": [
          "markdown",
          "rmd"
        ],
        "path": "./markdown.json"
      },
      {
        "language": "yaml",
        "path": "./docker/kubernetes.json"
      },
      {
        "language": "yaml",
        "path": "./docker/docker-compose.json"
      },
      {
        "language": "make",
        "path": "./make.json"
      },
      {
        "language": "dockerfile",
        "path": "./docker/docker_file.json"
      },
      {
        "language": [
          "powershell",
          "ps1"
        ],
        "path": "./PowerShell.json"
      }
    ]
  }
}
